<li>
  <%= link_to user, [:admin, user] %>
   <%= link_to "edit", edit_admin_user_path(user), class: "btn btn-mini" %>
  <% if user != current_user %>
     <%= link_to "delete", admin_user_path(user), method: :delete,
                  confirm: "Are you sure you want to delete this user?",
                  class: "btn btn-mini"%>
  <% end %>
</li>