<% provide(:title, "Booking #{@booking.id}") %>
<h1>Booking <%= @booking.id %></h1>
<h1><%= @booking.bucket %></h1>

<ol class="booking_details">
<% @booking.booking_details.each do |detail| %>
  <li>
    Charge <%= detail.rank %>:
    <%= detail.charge_id %> |
    <%= detail.alias %> |
    <%= detail.category %>
    <%#= ChargeType.find_by_id() %>
  </li>
<% end %>
</ol>

<%= link_to 'Edit', [:edit, @survey, @booking] %> |
<%= link_to 'Back to Bookings', survey_bookings_path(@survey) %>
